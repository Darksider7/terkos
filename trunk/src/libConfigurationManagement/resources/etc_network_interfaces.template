# /etc/network/interfaces -- configuration file for ifup(8), ifdown(8)
 
# The loopback interface
auto lo
iface lo inet loopback

# Wireless interfaces
@WLAN0_AUTOSTART_ON_BOOT@
iface wlan0 inet @WLAN0_ADDRESS_ALLOCATION_MODE@
	wireless_mode @WLAN0_WIRELESS_MODE@
	wireless_essid @WLAN0_SSID@

# Wired interfaces
auto eth0
iface eth0 inet dhcp
