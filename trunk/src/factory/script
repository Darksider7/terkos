set echo on
set comecho on
set senddelay 0.01
set com 460800n81
#
print "Waiting for VEXPro to finish booting..."
waitfor 200 "ogin:"
print "done^m^j"
send "root^m"
waitfor 5 "word:"
send "qwerk^m"
waitfor 5 "#"
send "killall LCDMenu^m"
waitfor 5 "#"
send "rmmod qemotor^m"
waitfor 5 "#"
send "rmmod qeinterrupt^m"
waitfor 5 "#"
send "/opt/bin/fpgac /opt/lib/vexprotest.bit^m"
waitfor 5 "#"
send "modprobe qeinterrupt^m"
waitfor 5 "#"
send "modprobe qemotor^m"
waitfor 5 "#"
send "/opt/bin/test^m"
waitfor 3600 "off"
print "Test complete^m^j"
