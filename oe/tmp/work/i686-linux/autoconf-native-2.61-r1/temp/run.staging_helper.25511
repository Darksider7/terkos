		fi
	fi
	rm -rf /home/dhuggins/Projects/TeRK/terkos/oe/tmp/work/i686-linux/autoconf-native-2.61-r1/temp-staging//home/dhuggins/Projects/TeRK/terkos/oe/tmp/staging/i686-linux/usr/share/man || true
	rm -rf /home/dhuggins/Projects/TeRK/terkos/oe/tmp/work/i686-linux/autoconf-native-2.61-r1/temp-staging//home/dhuggins/Projects/TeRK/terkos/oe/tmp/staging/i686-linux/usr/share/info || true
	autotools_stage_dir /home/dhuggins/Projects/TeRK/terkos/oe/tmp/work/i686-linux/autoconf-native-2.61-r1/temp-staging//home/dhuggins/Projects/TeRK/terkos/oe/tmp/staging/i686-linux/usr/share /home/dhuggins/Projects/TeRK/terkos/oe/tmp/staging/i686-linux/usr/share
	rm -rf /home/dhuggins/Projects/TeRK/terkos/oe/tmp/work/i686-linux/autoconf-native-2.61-r1/temp-staging

}

