		autotools_stage_dir /home/dhuggins/Projects/TeRK/terkos/oe/tmp/work/i686-linux/autoconf-native-2.61-r1/temp-staging//home/dhuggins/Projects/TeRK/terkos/oe/tmp/staging/i686-linux/bin /home/dhuggins/Projects/TeRK/terkos/oe/tmp/staging/i686-linux/bin
		autotools_stage_dir /home/dhuggins/Projects/TeRK/terkos/oe/tmp/work/i686-linux/autoconf-native-2.61-r1/temp-staging//home/dhuggins/Projects/TeRK/terkos/oe/tmp/staging/i686-linux/sbin /home/dhuggins/Projects/TeRK/terkos/oe/tmp/staging/i686-linux/sbin
		autotools_stage_dir /home/dhuggins/Projects/TeRK/terkos/oe/tmp/work/i686-linux/autoconf-native-2.61-r1/temp-staging//home/dhuggins/Projects/TeRK/terkos/oe/tmp/staging/i686-linux/usr/libexec /home/dhuggins/Projects/TeRK/terkos/oe/tmp/staging/i686-linux/usr/libexec
		if [ "/home/dhuggins/Projects/TeRK/terkos/oe/tmp/staging/i686-linux/usr/lib" != "/home/dhuggins/Projects/TeRK/terkos/oe/tmp/staging/i686-linux/usr/lib" ]; then
			# python puts its files in here, make sure they are staged as well
			autotools_stage_dir /home/dhuggins/Projects/TeRK/terkos/oe/tmp/work/i686-linux/autoconf-native-2.61-r1/temp-staging//home/dhuggins/Projects/TeRK/terkos/oe/tmp/staging/i686-linux/usr/lib /home/dhuggins/Projects/TeRK/terkos/oe/tmp/staging/i686-linux/usr/lib
		fi
	fi
	if [ -d /home/dhuggins/Projects/TeRK/terkos/oe/tmp/work/i686-linux/autoconf-native-2.61-r1/temp-staging//home/dhuggins/Projects/TeRK/terkos/oe/tmp/staging/i686-linux/usr/lib ]
	then
		olddir=`pwd`
		cd /home/dhuggins/Projects/TeRK/terkos/oe/tmp/work/i686-linux/autoconf-native-2.61-r1/temp-staging//home/dhuggins/Projects/TeRK/terkos/oe/tmp/staging/i686-linux/usr/lib
		las=$(find . -name \*.la -type f)
		cd $olddir
		echo "Found la files: $las"
		for i in $las
		do
			sed -e 's/^installed=yes$/installed=no/' \
			    -e '/^dependency_libs=/s,/home/dhuggins/Projects/TeRK/terkos/oe/tmp/work/i686-linux/autoconf-native-2.61-r1[[:alnum:]/\._+-]*/\([[:alnum:]\._+-]*.la\),/home/dhuggins/Projects/TeRK/terkos/oe/tmp/staging/i686-linux/usr/lib/\1,g' \
